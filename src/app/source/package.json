{
  "name": "trifled-terminal-wasm",
  "version": "0.1.0",
  "description": "WebAssembly terminal with CPU assembly support",
  "main": "pkg/source.js",
  "types": "pkg/source.d.ts",
  "scripts": {
    "build": "wasm-pack build --target web --out-dir pkg",
    "build:bundler": "wasm-pack build --target bundler --out-dir pkg",
    "build:nodejs": "wasm-pack build --target nodejs --out-dir pkg",
    "test": "wasm-pack test --chrome --firefox --headless"
  },
  "files": [
    "pkg"
  ],
  "devDependencies": {
    "wasm-pack": "^0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/trifledmatter-turing-complete-console.git"
  },
  "keywords": [
    "wasm",
    "webassembly",
    "terminal",
    "assembly",
    "cpu"
  ],
  "author": "Your Name",
  "license": "MIT"
} 